{
    "name": "@marceloglacial/snow-wind",
    "version": "1.0.0",
    "author": "Marcelo Glacial",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "yarn eslint && yarn format && next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier -w pages",
        "eslint": "eslint --fix pages/*.ts*",
        "husky-actions": "npm run format && npm run lint",
        "prepare": "husky install",
        "test": "concurrently \"yarn dev\" \"yarn run cypress open\"",
        "test:ci": "next build && concurrently \"yarn start\" \"cypress run --headless\""
    },
    "dependencies": {
        "@marceloglacial/brinca-ui": "^1.2.2",
        "@next/font": "13.4.6",
        "@types/node": "20.3.1",
        "@types/react": "18.2.13",
        "@types/react-dom": "18.2.6",
        "aos": "^2.3.4",
        "eslint": "8.43.0",
        "eslint-config-next": "13.4.6",
        "next": "13.4.6",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "swr": "^2.1.5",
        "typescript": "5.1.3"
    },
    "devDependencies": {
        "@types/aos": "^3.0.4",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@typescript-eslint/parser": "^5.60.0",
        "autoprefixer": "^10.4.14",
        "concurrently": "^8.2.0",
        "cypress": "^12.14.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "postcss": "^8.4.24",
        "prettier": "^2.8.8",
        "tailwindcss": "^3.3.2"
    }
}
