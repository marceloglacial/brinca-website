{
    "name": "@marceloglacial/snow-wind",
    "version": "1.0.0",
    "author": "Marcelo Glacial",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "yarn eslint && yarn format && next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier -w pages",
        "eslint": "eslint --fix pages/*.ts*",
        "husky-actions": "npm run format && npm run lint",
        "prepare": "husky install",
        "test": "concurrently \"yarn dev\" \"yarn run cypress open\"",
        "test:ci": "next build && concurrently \"yarn start\" \"cypress run --headless\""
    },
    "dependencies": {
        "@marceloglacial/brinca-ui": "^1.2.2",
        "@next/font": "13.2.3",
        "@types/node": "18.14.5",
        "@types/react": "18.0.28",
        "@types/react-dom": "18.0.11",
        "eslint": "8.35.0",
        "eslint-config-next": "13.2.3",
        "next": "13.2.3",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "typescript": "4.9.5"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.54.0",
        "autoprefixer": "^10.4.13",
        "concurrently": "^7.6.0",
        "cypress": "^12.7.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "postcss": "^8.4.21",
        "prettier": "^2.8.4",
        "tailwindcss": "^3.2.7"
    }
}
